import java.util.Scanner;
public class FactorialDiv {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		while(true){
			
			System.out.print("\nEnter \"K\": ");
			int k = input.nextInt();
			if(k<=1){
				System.out.print("\"K\" must be greater than 1 !!!");
				continue;
			}
		
			else{
				System.out.print("Enter \"N\": ");
				int n = input.nextInt();
				if(n<= k){
					System.out.print("\"N\" must be greater than  K ! Start again!");
					continue;
				}
				input.close();
		}
		
	}
	}
}
