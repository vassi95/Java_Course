import java.util.Scanner;
public class Division_plus_minus {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the first number: ");
		float num1 = input.nextFloat();
		System.out.println("Enter the first number: ");
		float num2 = input.nextFloat();
		if((num1 > 0 && num2 > 0) || (num1 < 0 && num2 < 0)){
			System.out.println("The result will be a POSITIVE number!");
		}
		else{
			System.out.println("The result will be a NEGATIVE number!");
		}
		double a1 = 0.0;
		double a2 =5;
		System.out.println("Enter the first number: " + a1/a2);
		System.out.println("Enter the first number: " + a2/a1);
		System.out.println("Enter the first number: " + a1/a1);
		input.close();

	}

}
