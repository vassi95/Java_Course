public class SymmetricNumbers {

	public static void main(String[] args) {
		for(int i =10; i<1000; i++){
		System.out.println(symmetric(i));
		}
	}
	
	public static Integer symmetric(int number){
		if(number>99){
			int hundreds = (number / 100);
			int remaining = number %10;
			if(hundreds == remaining){
				return number;
			}
		}
		else{
			int tens = (number / 10);
			int ones = number %10;
			if(ones == tens){
				return number;
			}
		}
		return null ;

	}

}
