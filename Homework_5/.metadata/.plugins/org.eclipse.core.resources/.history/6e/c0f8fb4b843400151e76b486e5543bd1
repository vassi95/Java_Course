import java.util.Scanner;

public class Permutations {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter a number:");
		int n = input.nextInt();
		int array[] = new int[n];
		for (int i = 1; i < n+1; i++) {
			array[i] = i;
		}

		printPermutation(array, n, 0);

		input.close();
	}

	public static void printPermutation(int[] array, int n, int i) {
		int j;
		// bottom of recursion //i-->start
		if (i == n) { // end of array->print
			System.out.print("(");
			for (j = 0; j < n; j++) {
				System.out.print(array[j] + ",");
			}
			System.out.print(")");
		} else {
			for (j = i; j < n; j++) {
				doSwap(array, i, j); // swap
				printPermutation(array, n, i + 1);
				doSwap(array, i, j); // swap them back
			}
		}

	}

	public static void doSwap(int[] array, int i, int j) {
		int temp = array[i];
		array[i] = array[j];
		array[j] = temp;

	}

}