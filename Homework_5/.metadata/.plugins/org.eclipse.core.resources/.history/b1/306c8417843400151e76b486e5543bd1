import java.util.Scanner;


public class Permutations {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.print("Enter a number: ");
		int n = input.nextInt();
		int[] array = new int[n];
		for(int i =1 ; i<=n; i++){
			array[i] = i;
		}
		System.out.println("\nThe permutations are: ");
		permutation(array, n, 0);
		input.close();
	}

		public static void permutation(int[] array, int beg ,int end ) {
		if(end==beg){
			for(int i = 0; i < array.length; i++){
				System.out.print(array[i] + " ");
			}
			System.out.println();
		}

		else {
			for (int i = end; i< beg; i++) {
				int temp = array[beg];
				array[beg] = array[end];
				array[end] = temp;
				permutation(array, beg, end+1);
				temp = array[beg];
				array[beg] = array[end];
				array[end] = temp;
			}
		}
	}

}
