package fromPresentation;

public class Throw {

	public static void main(String[] args) {

		User user1 = new User("blabla@abv.bg", "15sfss");
		User user2 = new User("asadada@abv.bg", "54ds32f");
		User user3 = new User("kndksd@mail.bg", "df7");

		try {
			
			System.out.println("Email: " + user1.getEmail());
			System.out.println("Email: " + user2.getEmail());
			System.out.println("Email: " + user3.getEmail());
			user1.getPassword();
			user2.getPassword();
			user3.getPassword();
		} catch (Exception exception) {
			System.out.println("An error: " + exception.getMessage());
		}

	}

}
